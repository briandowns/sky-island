{
    "release": "/tmp/11.1-RELEASE",
    "http_port": 3280,
    "go_version": "1.9.2",
    "filesystem": {
        "zfs_dataset": "zroot",
        "compression": false
    },
    "network": {
        "ip4": {
            "interface": "em0",
            "start_addr": "192.168.0.20",
            "mask": "255.255.255.0",
            "range": 220,
            "gateway": "192.168.0.1",
            "dns": [
                "4.2.2.1",
                "4.2.2.2"
            ]
        }
    },
    "jails": {
        "base_jail_dir": "/zroot/jails",
        "cache_default_expiration": "8h",
        "cache_purge_after": "24h",
        "children_max": 0,
        "monitoring_addr": "192.168.0.19"
    }
